.parse(a.tanggal_selesai) > Date.parse(b.tanggal_selesai)) { \r\n                        return 1\r\n                    }\r\n                    else if (Date.parse(a.tanggal_selesai) < Date.parse(b.tanggal_selesai)) {\r\n                        return -1\r\n                    }\r\n                    return 0;\r\n                })\r\n        }\r\n    }\r\n\r\n    const dataHandler = () => {\r\n        const reader = require(\"g-sheets-api\");\r\n        const readerOptions = {\r\n            sheetId: process.env.REACT_APP_SHEET_ID,\r\n            returnAllResults: false,\r\n        };\r\n        \r\n        reader(readerOptions, (results) => {\r\n            setAssignment(results)\r\n            localStorage.setItem('results', JSON.stringify(results))\r\n        })\r\n        .catch(err => {\r\n            let data = localStorage.getItem('results')\r\n            setAssignment(JSON.parse(data))\r\n        });\r\n    }\r\n\r\n    const saveHandler = (item) => {\r\n        let tempSaved = saved\r\n        if (!tempSaved.some(i => i['no'] == item['no'])) {\r\n            setRefresh(true)\r\n            tempSaved.push(item)\r\n            setSaved(tempSaved)\r\n            localStorage.setItem('saveData', JSON.stringify(tempSaved))\r\n            setTimeout(() => {\r\n                setRefresh(false)\r\n            }, 50)\r\n            \r\n        }\r\n        else {\r\n            setRefresh(true)\r\n            tempSaved.splice(tempSaved.indexOf(item), 1)\r\n            setSaved(tempSaved)\r\n            localStorage.setItem('saveData', JSON.stringify(tempSaved))\r\n            setTimeout(() => {\r\n                setRefresh(false)\r\n            }, 50)\r\n        }\r\n    }\r\n\r\n    useEffect(() => {\r\n            dataHandler()\r\n            const getData = setInterval(() => {\r\n                dataHandler()\r\n            }, 15000);\r\n            return () => clearInterval(getData);\r\n    }, [])\r\n\r\n\r\n\r\n    return (\r\n        !refresh && <div c